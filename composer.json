{
    "name": "tonybogdanov/class-config",
    "description": "Per-object configuration from class annotations",
    "type": "library",
    "require": {
        "php" : "^7.1",
        "doctrine/annotations": "1.6",
        "nette/php-generator": "3.1",
        "indigophp/doctrine-annotation-autoload": "^0.2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.5",
        "php-coveralls/php-coveralls": "2.0.0",
        "apigen/apigen": "dev-master"
    },
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Tony Bogdanov",
            "email": "tonybogdanov@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ClassConfig\\": "classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ClassConfig\\Test\\Sample\\": "cache/samples",
            "ClassConfig\\Test\\": "tests/classes/",
            "ClassConfig\\Cache\\": "cache/configs/"
        }
    }
}
